{
  "appId": "io.trackrone.app",
  "appName": "Trackr One",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#1a1a1a",
      "androidSplashResourceName": "splash"
