    "StatusBar": {
      "style": "dark"
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": true
    },
    "Haptics": {},
    "Device": {},
    "Network": {},
    "App": {}
  }
}
